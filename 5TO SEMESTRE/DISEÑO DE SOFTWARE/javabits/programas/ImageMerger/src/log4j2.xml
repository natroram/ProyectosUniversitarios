<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="debug" name="ImageMerger" packages="">
  <Appenders>
    <File name="RollingLog" fileName="logs/app.log">
      <PatternLayout>
        <Pattern>%d %p %c{1.} [%t] %m%n</Pattern>
      </PatternLayout>
    </File>
  </Appenders>
  <Loggers>
    <Root level="debug">
      <AppenderRef ref="RollingLog"/>
    </Root>
  </Loggers>
</Configuration>